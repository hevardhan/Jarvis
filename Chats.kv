MDScreen :
    chat_list:chat_list
    name : "chats"
    MDFloatLayout:
        md_bg_color:245/255,245/255,245/255,1
        size_hint_y:.11
        pos_hint: {"center_y":.95}
        halign:"center"
        MDLabel :
            text : "J.A.R.V.I.S"
            font_name : "DS-DIGIT.TTF"
            font_size : "35sp"
            pos_hint : {"center_x":.5,"center_y":.5}
            halign : "center"
            theme_text_color:"Custom"
            text_color : 53/255,56/255,60/255,1
    ScrollView:
        size_hint_y : .77
        pos_hint : {"x":0,"y":.116}
        do_scroll_x : False
        do_scroll_y : True
        BoxLayout : 
            id : chat_list
            orientation : "vertical"
            size : (root.width,root.height)
            height: self.minimum_height
            size_hint : None,None
            pos_hint : {'top':10} 
            cols :1
            spacing:3
    MDFloatingActionButton :
        icon : "microphone"
        pos_hint : {"center_x":.5,"center_y":.08}
        on_press: app.start()